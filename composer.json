{
    "name": "jumilla/erb2blade",
    "type": "framework-extension",
    "description": "convert .*.erb to .blade.php",
    "license": "MIT",
    "keywords": ["Laravel", "Laravel 5", "erb", "blade"],
    "homepage": "http://jumilla.me",
    "authors": [
        {
            "name": "Fumio Furukawa",
            "email": "fumio@jumilla.me"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/finder": "2.7.*",
        "illuminate/config": "5.0.*",
        "illuminate/console": "5.0.*",
        "illuminate/container": "5.0.*",
        "illuminate/contracts": "5.0.*",
        "illuminate/support": "5.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Jumilla\\Erb2Blade\\": "sources/"
        }
    },
    "minimum-stability": "stable"
}
